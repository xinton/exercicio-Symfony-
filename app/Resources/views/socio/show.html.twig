{% extends 'PetkoparaCrudGeneratorBundle::base.html.twig' %}

{% block title %}
Socio
{% endblock %}

{% block body %}

<div id="top" class="row">
    <div class="page-header">
        <h2>View socio <span class="glyphicon glyphicon-eye-open" aria-hidden="true"></span> </h2>
    </div>
</div>

<div class="row">
    <div class="col-md-6">
        <p><strong>Id</strong></p>
        <p>
            {{ socio.id }}
        </p>

    </div>
    <div class="col-md-6">
        <p><strong>Nome</strong></p>
        <p>
            {{ socio.nome }}
        </p>

    </div>
    <div class="col-md-6">
        <p><strong>Cpf</strong></p>
        <p>
            {{ socio.cpf }}
        </p>

    </div>
    <div class="col-md-6">
        <p><strong>Email</strong></p>
        <p>
            {{ socio.email }}
        </p>

    </div></div>

<hr/>


<div class="form-group">
    <a class="btn btn-default" href="{{ path('socio') }}">
        <span class="glyphicon glyphicon-list" aria-hidden="true"></span>
        Back to list
    </a>
    <a class="btn btn-primary" href="{{ path('socio_edit', { 'id':  socio.id }) }}">
        <span class="glyphicon glyphicon-pencil" aria-hidden="true"></span>
        Edit
    </a>
    <a class="btn btn-info" href="{{ path('socio_new') }}">
        <span class="glyphicon glyphicon-plus" aria-hidden="true"></span>
        New
    </a>
    <form action="{{ path('socio_delete', { 'id': socio.id }) }}" method="post" style="display: inline-block">
        <input type="hidden" name="_method" value="DELETE" />
        {{ form_widget(delete_form) }}
        <button class="btn btn-danger" type="submit" onclick="return confirm('Are you sure');">
            <span class="glyphicon glyphicon-remove" aria-hidden="true"></span>
            Delete
        </button>
    </form>
</div>


{% endblock %}
